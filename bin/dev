#!/usr/bin/env sh

# Provide some default values that allow the user to login.
# In a production like environment these are provided by the webserver/Shibboleth/LDAP
REMOTE_USER="${REMOTE_USER:=`whoami`@stanford.edu}"
ROLES="${ROLES:=sdr:etds-sul-staff}"
ORCID="${ORCID:=0000-0002-2100-6108}"

REMOTE_USER=$REMOTE_USER ROLES=$ROLES ORCID=$ORCID bin/overmind start -f Procfile.dev
